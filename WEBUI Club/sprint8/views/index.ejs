<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="initial-scale=1.0">
    <title>Express</title>
</head>

<body>
    <% if (data.length) { %>
        <% data.forEach(({ id, email, first_name, last_name, photo })=> { %>
            <h1>
                <%= id %>
            </h1>
            <p>
                <%= first_name %>
            </p>
            <p>
                <%= last_name %>
            </p>
            <p>
                <%= email %>
            </p>
            <img src="<%= photo %>" alt="<%= first_name %> photo">
            <% }); %>
                <% } %>

</body>

</html>